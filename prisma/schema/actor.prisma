model Actor {
  id        Int       @id @default(autoincrement())
  firstName String
  lastName  String
  bio       String?  // биография актёра
  photoUrl  String? 
  bornDate  DateTime?
  deathDate DateTime? 

  countryId Int   

  country   Country  @relation(fields: [countryId], references: [id])
  movies    Movie[]  
}
